{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "lib": [ "es2015", "dom" ],
    "module": "commonjs",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true
  },
  "exclude": ["test"]
}
